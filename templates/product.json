        "liquid_wUBR3i": {
          "type": "liquid",
          "settings": {
            "liquid": "{%- comment -%}\nCarrousel 3 images avec liens personnalisés\n{%- endcomment -%}\n\n{%- assign url_photo1 = \"https://cdn.shopify.com/s/files/1/0899/6821/4343/files/Capture_d_ecran_2025-07-16_231329.png?v=1752808055\" -%}\n{%- assign url_photo2 = \"https://cdn.shopify.com/s/files/1/0899/6821/4343/files/Capture_d_ecran_2025-07-16_231140.png?v=1752808055\" -%}\n{%- assign url_photo3 = \"https://cdn.shopify.com/s/files/1/0899/6821/4343/files/Capture_d_ecran_2025-07-16_231011.png?v=1752808055\" -%}\n\n<div class=\"simple-carousel\">\n  <div class=\"carousel-inner\">\n    {%- if url_photo1 != blank -%}\n    <div class=\"carousel-slide\">\n      <img src=\"{{ url_photo1 }}\" alt=\"Promotion 1\" loading=\"lazy\">\n    </div>\n    {%- endif -%}\n    \n    {%- if url_photo2 != blank -%}\n    <div class=\"carousel-slide\">\n      <img src=\"{{ url_photo2 }}\" alt=\"Promotion 2\" loading=\"lazy\">\n    </div>\n    {%- endif -%}\n    \n    {%- if url_photo3 != blank -%}\n    <div class=\"carousel-slide\">\n      <img src=\"{{ url_photo3 }}\" alt=\"Promotion 3\" loading=\"lazy\">\n    </div>\n    {%- endif -%}\n  </div>\n\n  <div class=\"carousel-dots\">\n    {%- if url_photo1 != blank -%}<span class=\"dot active\"></span>{%- endif -%}\n    {%- if url_photo2 != blank -%}<span class=\"dot\"></span>{%- endif -%}\n    {%- if url_photo3 != blank -%}<span class=\"dot\"></span>{%- endif -%}\n  </div>\n</div>\n\n<style>\n  .simple-carousel {\n    position: relative;\n    max-width: 600px;\n    margin: 0 auto;\n    overflow: hidden;\n  }\n  \n  .carousel-inner {\n    display: flex;\n    transition: transform 0.5s ease;\n  }\n  \n  .carousel-slide {\n    min-width: 100%;\n  }\n  \n  .carousel-slide img {\n    width: 100%;\n    display: block;\n  }\n  \n  .carousel-dots {\n    text-align: center;\n    margin-top: 10px;\n  }\n  \n  .dot {\n    display: inline-block;\n    width: 10px;\n    height: 10px;\n    border-radius: 50%;\n    background: #ccc;\n    margin: 0 5px;\n    cursor: pointer;\n  }\n  \n  .dot.active {\n    background: #333;\n  }\n</style>\n\n<script>\ndocument.addEventListener('DOMContentLoaded', function() {\n  const carousel = document.querySelector('.simple-carousel');\n  if (!carousel) return;\n  \n  const inner = carousel.querySelector('.carousel-inner');\n  const slides = carousel.querySelectorAll('.carousel-slide');\n  const dots = carousel.querySelectorAll('.dot');\n  \n  let currentIndex = 0;\n  const intervalTime = 3000;\n  let slideInterval;\n  \n  function moveToSlide(index) {\n    inner.style.transform = `translateX(-${index * 100}%)`;\n    currentIndex = index;\n    \n    dots.forEach(dot => dot.classList.remove('active'));\n    dots[currentIndex]?.classList.add('active');\n  }\n  \n  function nextSlide() {\n    const nextIndex = (currentIndex + 1) % slides.length;\n    moveToSlide(nextIndex);\n  }\n  \n  // Auto-rotation\n  function startCarousel() {\n    slideInterval = setInterval(nextSlide, intervalTime);\n  }\n  \n  // Click on dots\n  dots.forEach((dot, index) => {\n    dot.addEventListener('click', () => {\n      clearInterval(slideInterval);\n      moveToSlide(index);\n      startCarousel();\n    });\n  });\n  \n  // Pause on hover\n  carousel.addEventListener('mouseenter', () => clearInterval(slideInterval));\n  carousel.addEventListener('mouseleave', startCarousel);\n  \n  // Initialize\n  if (slides.length > 1) {\n    startCarousel();\n  } else if (dots.length > 0) {\n    carousel.querySelector('.carousel-dots').style.display = 'none';\n  }\n});\n</script>"
          }
